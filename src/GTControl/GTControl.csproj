  <ItemGroup>
    <Compile Update="Control\Page.cs">
      <SubType>UserControl</SubType>
    </Compile>
    <Compile Update="Control\Page.Designer.cs">
      <DependentUpon>Page.cs</DependentUpon>
    </Compile>
    <Compile Update="Control\PageBody.cs">
      <SubType>Component</SubType>
    </Compile>
    <Compile Update="Form\CreatePageForm.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Form\CreatePageForm.Designer.cs">
      <DependentUpon>CreatePageForm.cs</DependentUpon>
    </Compile>
    <Compile Update="Form\LayoutSettingForm.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Form\LayoutSettingForm.Designer.cs">
      <DependentUpon>LayoutSettingForm.cs</DependentUpon>
    </Compile>
    <Compile Update="Control\PageButton.cs">

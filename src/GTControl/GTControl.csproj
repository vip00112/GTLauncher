      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Form\NoteForm.Designer.cs">
      <DependentUpon>NoteForm.cs</DependentUpon>
    </Compile>
    <Compile Update="Model\Cell.cs" />
    <Compile Update="Model\Enums.cs" />
    <Compile Update="Properties\AssemblyInfo.cs" />
    <Compile Update="Model\Setting.cs" />
    <Compile Update="Form\SettingForm.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Form\SettingForm.Designer.cs">
      <DependentUpon>SettingForm.cs</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\GTCapture\GTCapture.csproj" />
    <ProjectReference Include="..\GTUtil\GTUtil.csproj" />
  </ItemGroup>

</Project>